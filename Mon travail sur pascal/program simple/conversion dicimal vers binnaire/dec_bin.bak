program bin_dic;

uses crt;
var n,p,bin :  longint ;
BEGIN
clrscr;
write( '  Enter a number  N = ' );
readln(n);
p:=1;
bin:= 0;
repeat
bin:= n mod 2 * p + bin ;
n := n div 2 ;
p:= p*10 ;

until ( n = 0 );
writeln ( 'The new number on the binary system  :' , bin );
writeln;
writeln;
writeln(    '         Press <   Enter  >  To quit' );
readln;

END.

